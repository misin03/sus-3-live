server {
    listen 80;
    server_name 3.36.70.72;

    location = /favicon.ico { access_log off; log_not_found off; }
    location /static/ {
        alias /home/ubuntu/sus-3-live/static/;
    }

    location / {
        include proxy_params;
        proxy_pass http://unix:/home/ubuntu/sus-3-live/gunicorn/django.sock;
    }
}